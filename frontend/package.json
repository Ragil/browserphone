{
  "name": "ragilphone",
  "version": "0.0.1",
  "description": "a phone in the browser",
  "main" : "src/main.jsx",
  "scripts": {
    "postinstall": "npm run build",
    "watch": "webpack --config webpack.local.config.js --watch --cache true --progress --colors",
    "server": "node server.js",
    "build": "webpack -p --config webpack.production.config.js",
    "test": "PHANTOMJS_BIN=./node_modules/.bin/phantomjs ./node_modules/karma/bin/karma start karma.config.js --single-run",
    "test-watch": "./node_modules/karma/bin/karma start karma.config.js --browsers Chrome --auto-watch",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "clean": "rm -rf build/*"
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "css-loader": "^0.12.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.1.0",
    "istanbul": "^0.3.7",
    "istanbul-instrumenter-loader": "^0.1.2",
    "jsx-loader": "^0.13.1",
    "karma": "^0.12.31",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-chrome-launcher": "~0.1",
    "karma-firefox-launcher": "~0.1.6",
    "karma-sinon-chai": "^0.3.0",
    "karma-webpack": "^1.5.0",
    "less-loader": "2.2.0",
    "phantomjs": "^1.9.16",
    "react-hot-loader": "^1.2.3",
    "style-loader": "^0.12.2",
    "url-loader": "^0.5.5",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "babel-core": "^5.0.8",
    "babel-loader": "^5.0.0",
    "bootstrap": "^3.3.4",
    "express": "^4.12.3",
    "jquery": "^2.1.4",
    "jquery.cookie": "1.4.1",
    "lodash": "^3.8.0",
    "moment": "^2.10.3",
    "react": "^0.13.1",
    "react-router": "^0.13.1",
    "simplewebrtc": "^1.15.4",
    "spin.js": "^2.1.0",
    "webpack": "^1.7.3"
  }
